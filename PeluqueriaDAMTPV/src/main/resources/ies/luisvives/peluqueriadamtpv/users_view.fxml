<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ies.luisvives.peluqueriadamtpv.controller.UsersController">
   <children>
      <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" StackPane.alignment="BOTTOM_CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="%text.userRegistration" textAlignment="CENTER" />
            <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                     <children>
                        <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" nodeOrientation="LEFT_TO_RIGHT" text="%text.username">
                           <VBox.margin>
                              <Insets bottom="13.0" top="13.0" />
                           </VBox.margin>
                        </Label>
                        <Label alignment="CENTER" contentDisplay="CENTER" layoutX="10.0" layoutY="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%text.name">
                           <VBox.margin>
                              <Insets bottom="13.0" />
                           </VBox.margin></Label>
                        <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="%text.surname">
                           <VBox.margin>
                              <Insets bottom="13.0" />
                           </VBox.margin></Label>
                        <Label layoutX="10.0" layoutY="60.0" text="%text.password">
                           <VBox.margin>
                              <Insets bottom="13.0" />
                           </VBox.margin></Label>
                        <Label layoutX="10.0" layoutY="85.0" text="%text.telephone">
                           <VBox.margin>
                              <Insets bottom="13.0" />
                           </VBox.margin></Label>
                        <Label layoutX="10.0" layoutY="110.0" text="%text.email">
                           <VBox.margin>
                              <Insets bottom="13.0" />
                           </VBox.margin></Label>
                        <Label layoutX="10.0" layoutY="135.0" text="%text.gender">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin></Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER_LEFT" HBox.hgrow="ALWAYS">
                     <children>
                        <TextField fx:id="usernameTextField">
                           <VBox.margin>
                              <Insets bottom="5.0" top="5.0" />
                           </VBox.margin>
                           <font>
                              <Font size="11.0" />
                           </font></TextField>
                        <TextField fx:id="nameTextField">
                           <font>
                              <Font size="11.0" />
                           </font>
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></TextField>
                        <TextField fx:id="surnameTextField">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></TextField>
                        <PasswordField fx:id="passwordTextField">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></PasswordField>
                        <TextField fx:id="telephoneTextField">
                           <VBox.margin>
                              <Insets bottom="5.0" />
                           </VBox.margin></TextField>
                        <TextField fx:id="emailTextField">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin></TextField>
                        <ChoiceBox fx:id="gender_choice_box" prefWidth="150.0">
                           <VBox.margin>
                              <Insets top="5.0" />
                           </VBox.margin></ChoiceBox>
                     </children>
                     <HBox.margin>
                        <Insets top="10.0" />
                     </HBox.margin>
                  </VBox>
                  <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" HBox.hgrow="ALWAYS">
                     <children>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                           <VBox.margin>
                              <Insets />
                           </VBox.margin></ImageView>
                        <TextField fx:id="imageTextField">
                           <VBox.margin>
                              <Insets top="7.0" />
                           </VBox.margin></TextField>
                        <Button fx:id="userButtonUploadImage" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="%text.uploadImg" />
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
            <Button mnemonicParsing="false" onAction="#insertUser" text="%text.createUser" />
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <TableView fx:id="listUsers" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <columns>

                    </columns>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </HBox.margin>

                  </TableView>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0">
                     <children>
                        <Button mnemonicParsing="false" text="%text.modify" VBox.vgrow="ALWAYS" />
                        <Button mnemonicParsing="false" onAction="#onTableItemUser" text="%text.view" VBox.vgrow="ALWAYS" />
                        <Button mnemonicParsing="false" onAction="#deleteUser" text="%text.delete" VBox.vgrow="ALWAYS" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
