<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<StackPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ies.luisvives.peluqueriadamtpv.controller.UsersController">
   <children>
      <VBox alignment="TOP_CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" StackPane.alignment="BOTTOM_CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="Alta Usuario" textAlignment="CENTER" />
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
               <children>
                  <VBox alignment="CENTER_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <Label contentDisplay="RIGHT" text="Username:">
                           <graphic>
                              <TextField fx:id="userTextFieldUsername" alignment="CENTER_RIGHT" />
                           </graphic>
                        </Label>
                        <Label contentDisplay="RIGHT" layoutX="10.0" layoutY="10.0" text="Name:">
                           <graphic>
                              <TextField fx:id="userTextFieldName" />
                           </graphic>
                        </Label>
                        <Label contentDisplay="RIGHT" layoutX="10.0" layoutY="35.0" text="Sur Name:">
                           <graphic>
                              <TextField fx:id="userTextFieldSurName" />
                           </graphic>
                        </Label>
                        <Label contentDisplay="RIGHT" layoutX="10.0" layoutY="60.0" text="Password:">
                           <graphic>
                              <TextField fx:id="userTextFieldPassword" />
                           </graphic>
                        </Label>
                        <Label contentDisplay="RIGHT" layoutX="10.0" layoutY="85.0" text="Telephone:">
                           <graphic>
                              <TextField fx:id="userTextFieldTelephone" />
                           </graphic>
                        </Label>
                        <Label contentDisplay="RIGHT" layoutX="10.0" layoutY="110.0" text="E-mail:">
                           <graphic>
                              <TextField fx:id="userTextFieldEmail" />
                           </graphic>
                        </Label>
                        <Label contentDisplay="RIGHT" layoutX="10.0" layoutY="135.0" text="Gender:">
                           <graphic>
                              <ChoiceBox prefWidth="150.0" />
                           </graphic>
                        </Label>
                     </children>
                  </VBox>
                  <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                     <children>
                        <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                        <TextField fx:id="userTextFieldImagePath" />
                        <Button fx:id="userButtonUploadImage" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" text="Upload image" />
                     </children>
                  </VBox>
               </children>
            </HBox>
            <Button mnemonicParsing="false" text="Create User" />
            <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
               <children>
                  <TableView maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <columns>
                      <TableColumn maxWidth="1.7976931348623157E308" text="Username" />
                      <TableColumn maxWidth="1.7976931348623157E308" minWidth="-1.0" text="Name" />
                        <TableColumn maxWidth="1.7976931348623157E308" minWidth="-1.0" text="SurName" />
                        <TableColumn maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Telephone" />
                        <TableColumn maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Email" />
                        <TableColumn maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Gender" />
                        <TableColumn maxWidth="1.7976931348623157E308" minWidth="-Infinity" text="Image" />
                    </columns>
                     <HBox.margin>
                        <Insets bottom="10.0" left="10.0" top="10.0" />
                     </HBox.margin>
                  </TableView>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <Button mnemonicParsing="false" text="Modify" />
                        <Button mnemonicParsing="false" text="View" />
                        <Button mnemonicParsing="false" text="Delete" />
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</StackPane>
